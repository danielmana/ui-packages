{
  "name": "@danielmana/ui-components",
  "version": "0.1.2",
  "private": false,
  "author": "danielmana",
  "description": "A library of React UI components.",
  "main": "./src/index.ts",
  "keywords": [
    "react",
    "react-component",
    "design-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielmana/ui-packages.git",
    "directory": "packages/ui-components"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielmana/ui-packages/issues"
  },
  "homepage": "https://verdant-klepon-5d2a5e.netlify.app/",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/mui"
  },
  "scripts": {
    "build": "yarn build:modern && yarn build:node && yarn build:stable && yarn build:types && yarn build:copy-files",
    "build:modern": "node ../../scripts/build modern",
    "build:node": "node ../../scripts/build node",
    "build:stable": "node ../../scripts/build stable",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "build:types": "node ../../scripts/buildTypes",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "release": "yarn build && npm publish build",
    "test": "cd ../../ && cross-env NODE_ENV=test mocha 'packages/ui-components/**/*.test.{js,ts,tsx}'",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{spec,d}.{ts,tsx}\" && tsc -p tsconfig.json",
    "typescript:module-augmentation": "node scripts/testModuleAugmentation.js"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@danielmana/ui-core": "^0.1.13",
    "clsx": "^1.2.1",
    "prop-types": "^15.8.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "gitHead": "637d597a805bb7a94053a5bb6e24eea2d92be768"
